<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhaozengzhi.wedding.dao.IBlessDao">
    <insert id="addBless" parameterType="String">
        insert into tbl_bless(user) values(#{user});
    </insert>
    <select id="getBless" resultType="String">
        select * from tbl_bless;
    </select>
</mapper>